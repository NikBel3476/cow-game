{
  "name": "cow-game",
  "version": "0.0.1",
  "description": "Задача в игре: с помощью стрелок довести корову коричневого цвета до кубка",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clean": "rescript clean -with-deps",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:js": "npx eslint **/*.{js,ts,jsx,tsx}",
    "lint:css": "npx stylelint \"**/*.css\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikBel3476/cow-game.git"
  },
  "keywords": [],
  "author": "NikBel3476",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NikBel3476/cow-game/issues"
  },
  "homepage": "https://github.com/NikBel3476/cow-game#readme",
  "lint-staged": {
    "!(*.bs).{js,jsx}": [
      "prettier --write"
    ],
    "!(*.gen).{ts,tsx}": [
      "prettier --write"
    ],
    "*.res": [
      "rescript format"
    ]
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "phaser": "^3.60.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@jihchi/vite-plugin-rescript": "^5.3.0",
    "@rescript/react": "^0.11.0",
    "@types/node": "^20.4.8",
    "@types/react": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.1",
    "gentype": "^4.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "rescript": "^10.1.4",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.8"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
