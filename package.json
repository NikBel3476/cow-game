{
  "name": "cow-game",
  "version": "0.0.1",
  "description": "Задача в игре: с помощью стрелок довести корову коричневого цвета до кубка",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clean": "rescript clean -with-deps",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:js": "npx eslint .",
    "lint:css": "npx stylelint \"**/*.css\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikBel3476/cow-game.git"
  },
  "keywords": [],
  "author": "NikBel3476",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NikBel3476/cow-game/issues"
  },
  "homepage": "https://github.com/NikBel3476/cow-game#readme",
  "dependencies": {
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "!(*.bs).{js,jsx}": [
      "prettier --write"
    ],
    "!(*.gen).{ts,tsx}": [
      "prettier --write"
    ],
    "*.res": [
      "rescript format"
    ]
  },
  "devDependencies": {
    "@jihchi/vite-plugin-rescript": "^5.3.0",
    "@rescript/react": "^0.11.0-rc.3",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "gentype": "^4.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rescript": "^10.1.0",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
